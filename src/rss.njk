---
permalink: /rss.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:content="http://purl.org/rss/1.0/modules/content/">
  <channel>
    <title>Kyrtin Atreides - Essays</title>
    <link>https://kyrtinatreides.com/</link>
    <description>Essays and insights on AI, technology, and society</description>
    <language>en</language>
    <pubDate>{{ collections.essays[0].date | dateToRfc822 }}</pubDate>
    <lastBuildDate>{{ collections.essays[0].date | dateToRfc822 }}</lastBuildDate>
    
    {%- for essay in collections.essays %}
    <item>
      <title>{{ essay.data.title | escape }}</title>
      <link>{{ essay.url | absoluteUrl('https://kyrtinatreides.com') }}</link>
      <description>{{ essay.templateContent | htmlToAbsoluteUrls('https://kyrtinatreides.com') | escape }}</description>
      <pubDate>{{ essay.date | dateToRfc822 }}</pubDate>
      <dc:creator>Kyrtin Atreides</dc:creator>
      <guid>{{ essay.url | absoluteUrl('https://kyrtinatreides.com') }}</guid>
    </item>
    {%- endfor %}
  </channel>
</rss> 